<odoo>
    <data>
        <record id='instafeed_action' model='ir.actions.act_window'>
            <field name="name">Instafeed</field>
            <field name="res_model">instafeed</field>
            <field name="view_mode">tree,form,kanban</field>
        </record>
        <record id="instafeed_view_form" model="ir.ui.view">
            <field name="name">Instafeed form</field>
            <field name="model">instafeed</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="store_id"/>
                            <field name="feed_title"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="instafeed_view_tree" model="ir.ui.view">
            <field name="name">Instafeed tree</field>
            <field name="model">instafeed</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="feed_title"/>
                    <field name="store_id"/>
                </tree>
            </field>
        </record>
        <record id='instafeed_user_action' model='ir.actions.act_window'>
            <field name="name">Instafeed User</field>
            <field name="res_model">instafeed.user</field>
            <field name="view_mode">tree,form,kanban</field>
        </record>
        <record id="instafeed_user_view_form" model="ir.ui.view">
            <field name="name">Instafeed user form</field>
            <field name="model">instafeed.user</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="store_id"/>
                            <field name="user_id"/>
                            <field name="instagram_user"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="instafeed_user_view_tree" model="ir.ui.view">
            <field name="name">Instafeed tree</field>
            <field name="model">instafeed.user</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="store_id"/>
                    <field name="user_id"/>
                </tree>
            </field>
        </record>

        <menuitem name="Instafeed" id="instafeed_base_menu"/>
        <menuitem name="Instafeed" id="instafeed_menu" action="instafeed_action" parent="instafeed_base_menu"
                  sequence="15"/>
        <menuitem name="Instafeed user" id="instafeed_user_menu" action="instafeed_user_action"
                  parent="instafeed_base_menu"
                  sequence="20"/>
    </data>
</odoo>