<odoo>
    <data>
        <record id='post_action' model='ir.actions.act_window'>
            <field name="name">post User</field>
            <field name="res_model">instagram.post</field>
            <field name="view_mode">tree,form,kanban</field>
        </record>
        <record id="post_view_form" model="ir.ui.view">
            <field name="name">post user form</field>
            <field name="model">instagram.post</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="products"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="post_view_tree" model="ir.ui.view">
            <field name="name">post tree</field>
            <field name="model">instagram.post</field>
            <field name="arch" type="xml">
                <tree>
                            <field name="products"/>
                </tree>
            </field>
        </record>

        <menuitem name="post" id="post_menu" action="post_action" parent="instafeed_base_menu"
                  sequence="175"/>
    </data>
</odoo>